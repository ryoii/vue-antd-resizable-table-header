    "package": "vue-cli-service build --target lib --name vue-antd-draggable-table-header src/VueAntdResizableTableHeader.js"
  },
  "description": "An table plugin for table component in antd for vue.",
  "keywords": [
    "vue3",
    "ant design",
    "compoents",
    "table",
    "draggable"
  ],
  "homepage": "https://github.com/ryoii/vue-antd-resizable-table-header",
  "license": "MIT",
  "contributors": [
    "ryoii"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com:ryoii/vue-antd-resizable-table-header.git"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.12.0",
    "less-loader": "^10.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "ant-design-vue": "^2.2.2"
  },
  "eslintConfig": {
